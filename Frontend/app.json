{
  "expo": {
    "name": "nearbii",
    "slug": "nearbii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Nearbii to access your device's location",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow Nearbii to access your device's location"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "babel": {
          "include": [
            "react-router-native"
          ]
        }
      }
    },
    "description": "An early stage development social media project, involving anonymous posts that are accessible based on location.",
    "githubUrl": "https://github.com/nearbii/nearbii"
  }
}
